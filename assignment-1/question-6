// ðŸ’¡ Given an integer array nums, return true if any value appears at least twice in the array, and return false if every element is distinct.

// **Example 1:**
// Input: nums = [1,2,3,1]

// Output: true 

#include<bits/stdC++.h>
using namespace std;
bool check(vector<int>&vec)
{
    set<int>st;
    for(auto it:vec)
    {
        st.insert(it);
    }
    if(st.size()==vec.size())
    return true;
    return false;
}

int main(){
   vector<int>vec;
   int n;
   cout<<"enter the size of array"<<endl;
   cin>>n;
   for(int i=0;i<n;i++)
   {
     int temp;
     cin>>temp;
     vec.push_back(temp);
   }
   cout<<endl;
   cout<<check(vec)<<endl;
   return 0;
}
